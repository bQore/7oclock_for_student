<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
        >

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" android:background="@drawable/ll_main_view_box"
            android:layout_marginBottom="5dp" android:layout_marginTop="5dp">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal"
                android:paddingTop="5dp" android:paddingBottom="10dp" android:layout_weight="1"
                android:id="@+id/ll_testpaper_result_rank_u1" android:visibility="gone">
            <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:id="@+id/iv_testpaper_result_rank_pic1"
                        android:src="@drawable/default_user"
                        android:layout_gravity="top|left" android:layout_marginBottom="5dp"
                        android:layout_marginRight="5dp" android:layout_marginTop="8dp"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView" android:src="@drawable/testpaper_result_rank_crown"
                        android:layout_gravity="center_horizontal" android:layout_marginRight="3dp"/>
                <net.sevenoclock.mobile.customobj.FontTextView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:text="1"
                        android:id="@+id/tv_testpaper_result_rank_rank1" android:background="#f39c12"
                        android:textColor="#FFFFFF" android:gravity="center_vertical|center_horizontal"
                        android:textSize="12sp" android:layout_gravity="bottom|right"/>
            </FrameLayout>
            <net.sevenoclock.mobile.customobj.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="홍길동"
                    android:id="@+id/tv_testpaper_result_rank_name1" android:textSize="15sp" android:textColor="#333333"
                    android:textStyle="bold" android:layout_marginTop="3dp"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal"
                android:paddingTop="5dp" android:paddingBottom="10dp" android:layout_weight="1"
                android:id="@+id/ll_testpaper_result_rank_u2" android:visibility="gone">
            <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:id="@+id/iv_testpaper_result_rank_pic2"
                        android:src="@drawable/default_user"
                        android:layout_gravity="top|left" android:layout_marginBottom="5dp"
                        android:layout_marginRight="5dp" android:layout_marginTop="8dp"/>
                <net.sevenoclock.mobile.customobj.FontTextView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:text="2"
                        android:id="@+id/tv_testpaper_result_rank_rank2" android:background="#f39c12"
                        android:textColor="#FFFFFF" android:gravity="center_vertical|center_horizontal"
                        android:textSize="12sp" android:layout_gravity="bottom|right"/>
            </FrameLayout>
            <net.sevenoclock.mobile.customobj.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="홍길동"
                    android:id="@+id/tv_testpaper_result_rank_name2" android:textSize="15sp" android:textColor="#333333"
                    android:textStyle="bold" android:layout_marginTop="3dp"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="fill_parent" android:gravity="center_vertical|center_horizontal"
                android:paddingTop="5dp" android:paddingBottom="10dp" android:layout_weight="1"
                android:id="@+id/ll_testpaper_result_rank_u3" android:visibility="gone">
            <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:id="@+id/iv_testpaper_result_rank_pic3"
                        android:src="@drawable/default_user"
                        android:layout_gravity="top|left" android:layout_marginBottom="5dp"
                        android:layout_marginRight="5dp" android:layout_marginTop="8dp"/>
                <net.sevenoclock.mobile.customobj.FontTextView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:text="3"
                        android:id="@+id/tv_testpaper_result_rank_rank3" android:background="#f39c12"
                        android:textColor="#FFFFFF" android:gravity="center_vertical|center_horizontal"
                        android:textSize="12sp" android:layout_gravity="bottom|right"/>
            </FrameLayout>
            <net.sevenoclock.mobile.customobj.FontTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="홍길동"
                    android:id="@+id/tv_testpaper_result_rank_name3" android:textSize="15sp" android:textColor="#333333"
                    android:textStyle="bold" android:layout_marginTop="3dp"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" android:background="@drawable/ll_main_view_box"
            android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:gravity="center_vertical">
        <net.sevenoclock.mobile.customobj.FontTextView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:text="1"
                android:id="@+id/tv_testpaper_result_rank_myrank" android:layout_margin="10dp" android:background="#34495e"
                android:textColor="#FFFFFF" android:gravity="center_vertical|center_horizontal"
                android:textSize="12sp"/>
        <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/iv_testpaper_result_rank_mypic"
                android:src="@drawable/default_user" android:layout_marginTop="10dp"
                android:layout_marginRight="10dp" android:layout_marginBottom="10dp"/>
        <net.sevenoclock.mobile.customobj.FontTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="홍길동"
                android:id="@+id/tv_testpaper_result_rank_myname" android:textSize="15sp" android:textColor="#333333"
                android:textStyle="bold"/>
        <net.sevenoclock.mobile.customobj.FontTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="100점"
                android:id="@+id/tv_testpaper_result_rank_myscore" android:gravity="right" android:textColor="#999999"
                android:textSize="25sp" android:textStyle="bold" android:layout_marginRight="20dp"/>
    </LinearLayout>
</LinearLayout>